[MASTER]
extension-pkg-whitelist=lxml
ignored-modules=cv2,tesserocr

[MESSAGES CONTROL]
disable =
    ungrouped-imports,
    fixme,
    bad-continuation,
    missing-docstring,
    no-self-use,
    too-many-arguments,
    superfluous-parens,
    invalid-name,
    line-too-long,
    too-many-locals,
    too-few-public-methods,
